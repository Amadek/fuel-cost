{
  "name": "fuel-cost-api-logic",
  "version": "1.0.0",
  "description": "Project aims to store fuel consumption of your car in easy way by simple API.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Amadek/fuel-cost-api-logic.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",

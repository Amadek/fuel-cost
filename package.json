  "license": "MIT",
  "dependencies": {
    "@amadek/js-sdk": "^1.1.3",
    "axios": "^0.19.2",
    "cross-env": "^7.0.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "http-errors": "^1.7.3",
    "mongodb": "^3.5.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.46",
    "@types/http-errors": "^1.6.3",
    "@types/mongodb": "^3.5.14",
    "concurrently": "^5.2.0",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.2",
    "typescript": "^3.8.3"
  }
